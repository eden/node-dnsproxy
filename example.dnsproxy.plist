<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>example.dnsproxy</string>
    <key>EnvironmentVariables</key>
    <dict>
	    <key>PATH</key>
	    <string>/usr/bin:/usr/local/bin</string>
    </dict>
    <key>ProgramArguments</key>
    <array>
      <string>bash</string>
      <string>-c</string>
      <string>/usr/local/bin/dnsproxy -f /usr/local/etc/dnsproxy.conf</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
  </dict>
</plist>
